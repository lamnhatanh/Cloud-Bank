      <Label layoutX="196.0" layoutY="91.0" text="Welcome to Cloud Bank" textFill="WHITE">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button fx:id="withdrawBtn" layoutX="163.0" layoutY="208.0" mnemonicParsing="false" onAction="#withdrawal" prefHeight="39.0" prefWidth="249.0" text="Withdrawal">
