            <Font name="Bell MT Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="BIBtn" layoutX="163.0" layoutY="386.0" mnemonicParsing="false" onAction="#balanceInquiry" prefHeight="25.0" prefWidth="249.0" text="Balance Inquiry">
